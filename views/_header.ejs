<header>
    This is a header
    <ul>
        <% if(locals.user){ %>
            <li>
                <a href="/users/profile"></a>
                <%= user.name %>
            </li>
            <li>
                <a href="/users/sign-out">Sign Out</a>
            </li>
        <% }else{ %>
            <li>
                <a href="/users/sign-in">Sign In</a>
            </li>
            <li>
                <a href="/users/sign-up">Sign Up</a>
            </li>
        <% } %>
    </ul>
</header>